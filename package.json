{
  "publisher": "Debbl",
  "name": "vscode-toggle-bool",
  "displayName": "Toggle Bool",
  "version": "0.0.4",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "description": "quickly toggle boolean value at the cursor position",
  "author": "Brendan Dash <me@aiwan.run>",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/vscode-toggle-bool#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Debbl/vscode-toggle-bool"
  },
  "bugs": {
    "url": "https://github.com/Debbl/vscode-toggle-bool/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "toggleBool.toggle",
        "title": "Toggle Bool: Toggle Bool"
      }
    ]
  },
  "scripts": {
    "build": "tsup",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@debbl/eslint-config": "^3.0.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^2.23.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "vitest": "^1.2.2"
  }
}
